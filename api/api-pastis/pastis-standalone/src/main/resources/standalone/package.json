    "@babel/compat-data": "^7.8.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/d3": "^3.5.43",
    "@types/uuid": "^7.0.0",
    "angular-svg-icon": "^11.2.0",
