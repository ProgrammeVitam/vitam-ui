{
  "name": "vitamui-ui",
  "version": "5.4.0-SNAPSHOT",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve --disable-host-check --ssl --ssl-key ../../dev-deployment/environments/certs/server/hosts/localhost/reverse.key --ssl-cert ../../dev-deployment/environments/certs/server/hosts/localhost/reverse.crt",
    "start:demo": "npm run start -- demo --proxy-config proxy.conf.json",
    "start:portal": "npm run start -- portal --proxy-config proxy.conf.json",
    "start:portal:gateway": "npm run start -- portal --proxy-config proxy-gateway.conf.js",
    "start:portal:gateway-next": "npm run start -- portal --proxy-config proxy-gateway-next.conf.js",
    "start:design-system": "npm run start -- design-system",
    "start:identity": "npm run start -- identity --proxy-config proxy.conf.json",
    "start:identity:gateway": "npm run start -- identity --proxy-config proxy-gateway.conf.js",
    "start:identity:gateway-next": "npm run start -- identity --proxy-config proxy-gateway-next.conf.js",
    "start:pastis": "npm run start -- pastis --proxy-config proxy.conf.json",
    "start:pastis:gateway": "npm run start -- pastis --proxy-config proxy-gateway.conf.js",
    "start:pastis:gateway-next": "npm run start -- pastis --proxy-config proxy-gateway-next.conf.js",
    "start:pastis:standalone": "npm run start -- pastis --configuration standalone",
    "start:referential": "npm run start -- referential --proxy-config proxy.conf.json",
    "start:referential:gateway": "npm run start -- referential --proxy-config proxy-gateway.conf.js",
    "start:referential:gateway-next": "npm run start -- referential --proxy-config proxy-gateway-next.conf.js",
    "start:ingest": "npm run start -- ingest --proxy-config proxy.conf.json",
    "start:ingest:gateway": "npm run start -- ingest --proxy-config proxy-gateway.conf.js",
    "start:ingest:gateway-next": "npm run start -- ingest --proxy-config proxy-gateway-next.conf.js",
    "start:archive-search": "npm run start -- archive-search --proxy-config proxy.conf.json",
    "start:archive-search:gateway": "npm run start -- archive-search --proxy-config proxy-gateway.conf.js",
    "start:archive-search:gateway-next": "npm run start -- archive-search --proxy-config proxy-gateway-next.conf.js",
    "start:collect": "npm run start -- collect --proxy-config proxy.conf.json",
    "start:collect:gateway": "npm run start -- collect --proxy-config proxy-gateway.conf.js",
    "start:collect:gateway-next": "npm run start -- collect --proxy-config proxy-gateway-next.conf.js",
    "build": "ng build --prod --build-optimizer=false --optimization=false",
    "postinstall": "ngcc",
    "build:prod": "export NODE_OPTIONS=--max_old_space_size=2048; ng build --prod --output-path ../../../target/www",
    "build:dev": "ng build --prod --build-optimizer=false --optimization=false",
    "build:demo": "ng build demo --prod",
    "build:allModules": "concurrently --max-processes 4 'npm:build:portal' 'npm:build:identity' 'npm:build:referential' 'npm:build:archive-search' 'npm:build:pastis' 'npm:build:collect' 'npm:build:ingest' 'npm:build:design-system'",
    "build:portal": "export NODE_OPTIONS=--max_old_space_size=2048; ng build portal --prod",
    "build:identity": "export NODE_OPTIONS=--max_old_space_size=2048; ng build identity --prod --base-href=/identity/ --deploy-url=/identity/",
    "build:ingest": "export NODE_OPTIONS=--max_old_space_size=2048; ng build ingest --prod --base-href=/ingest/ --deploy-url=/ingest/",
    "build:archive-search": "export NODE_OPTIONS=--max_old_space_size=2048; ng build archive-search --prod --base-href=/archive-search/ --deploy-url=/archive-search/",
    "build:referential": "export NODE_OPTIONS=--max_old_space_size=2048; ng build referential --prod --base-href=/referential/ --deploy-url=/referential/",
    "build:pastis": "export NODE_OPTIONS=--max_old_space_size=2048; ng build pastis --prod --base-href=/pastis/ --deploy-url=/pastis/",
    "build:pastis-standalone": "NODE_OPTIONS=--max_old_space_size=2048 ng build --configuration=standalone --project=pastis",
    "build:collect": "export NODE_OPTIONS=--max_old_space_size=2048; ng build collect --prod --base-href=/collect/ --deploy-url=/collect/",
    "build:design-system": "export NODE_OPTIONS=--max_old_space_size=2048; ng build design-system --prod --base-href=/design-system/ --deploy-url=/design-system/",
    "analyze-portal": "ng build portal --stats-json --prod ; webpack-bundle-analyzer dist/portal/stats.json",
    "analyze-identity": "ng build identity --stats-json --prod ; webpack-bundle-analyzer dist/identity/stats.json",
    "analyze-referential": "ng build referential --stats-json --prod ; webpack-bundle-analyzer dist/referential/stats.json",
    "analyze-ingest": "ng build ingest --stats-json --prod ; webpack-bundle-analyzer dist/ingest/stats.json",
    "analyze-archive-search": "ng build archive-search --stats-json --prod ; webpack-bundle-analyzer dist/archive-search/stats.json",
    "analyze-collect": "ng build collect --stats-json --prod ; webpack-bundle-analyzer dist/collect/stats.json",
    "test": "ng test --code-coverage",
    "ci:test": "concurrently --max-processes 4 'npm:test:*'",
    "test:archive-search": "ng test archive-search --code-coverage ",
    "test:collect": "ng test collect --code-coverage",
    "test:identity": "ng test identity --code-coverage",
    "test:ingest": "ng test ingest --code-coverage",
    "test:pastis": "ng test pastis --code-coverage",
    "test:portal": "ng test portal --code-coverage",
    "test:referential": "ng test referential --code-coverage",
    "test:vitamui-library": "ng test vitamui-library",
    "lint": "ng lint --format=stylish",
    "lint:demo": "ng lint demo --format=stylish",
    "lint:identity": "ng lint identity --format=stylish",
    "lint:ingest": "ng lint ingest --format=stylish",
    "lint:portal": "ng lint portal --format=stylish",
    "lint:archive-search": "ng lint archive-search --format=stylish",
    "lint:referential": "ng lint referential --format=stylish",
    "lint:collect": "ng lint collect --format=stylish",
    "lint:pastis": "ng lint pastis --format=stylish",
    "lint:vitamui-library": "ng lint vitamui-library --format=stylish",
    "e2e": "ng e2e",
    "check:demo": "npm run lint:demo && npm run build:demo",
    "check:portal": "npm run lint:portal && npm run test:portal && npm run build:portal",
    "check:identity": "npm run lint:identity && npm run test:identity && npm run build:identity",
    "extract-translations": "ngx-translate-extract --input ./projects/portal/src --output ./projects/portal/src/assets/i18n/*.json --sort --format namespaced-json --key-as-default-value",
    "check:referential": "npm run lint:referential && npm run test:referential && npm run build:referential",
    "check:archive-search": "npm run lint:archive-search && npm run test:archive-search && npm run build:archive-search",
    "check:collect": "npm run lint:collect && npm run test:collect && npm run build:collect",
    "copy-scss:vitamui-library": "mkdir -p dist/ && cp -a ./projects/vitamui-library/src/sass ./dist/",
    "build:vitamui-library": "ng build vitamui-library",
    "watch:vitamui-library": "ng build vitamui-library --watch",
    "publish:portal": "cp projects/portal/package.json dist/portal/package.json && npm publish dist/portal/",
    "publish:identity": "cp projects/identity/package.json dist/identity/package.json && npm publish dist/identity/",
    "publish:ingest": "cp projects/ingest/package.json dist/ingest/package.json && npm publish dist/ingest/",
    "publish:pastis": "cp projects/pastis/package.json dist/pastis/package.json && npm publish dist/pastis/",
    "publish:collect": "cp projects/collect/package.json dist/collect/package.json && npm publish dist/collect/",
    "publish:archive-search": "cp projects/archive-search/package.json dist/archive-search/package.json && npm publish dist/archive-search/",
    "publish:referential": "cp projects/referential/package.json dist/referential/package.json && npm publish dist/referential/",
    "publish:all": "npm run publish:portal &&  npm run publish:identity  && npm run publish:referential && npm run publish:archive-search && npm run publish:collect  && npm run publish:pastis && npm run publish:ingest",
    "package:identity": "mkdir -p dist/identity/ && cp projects/identity/package.json dist/identity/package.json && npm pack dist/identity/",
    "package:archive-search": "mkdir -p dist/archive-search/ && cp projects/archive-search/package.json dist/archive-search/package.json && npm pack dist/archive-search/",
    "package:portal": "mkdir -p dist/portal/ && cp projects/portal/package.json dist/portal/package.json && npm pack dist/portal/",
    "package:collect": "mkdir -p dist/collect/ && cp projects/collect/package.json dist/collect/package.json && npm pack dist/collect/",
    "package:ingest": "mkdir -p dist/ingest/ && cp projects/ingest/package.json dist/ingest/package.json && npm pack dist/ingest/",
    "package:pastis": "mkdir -p dist/pastis/ && cp projects/pastis/package.json dist/pastis/package.json && npm pack dist/pastis/",
    "package:referential": "mkdir -p dist/referential/ && cp projects/referential/package.json dist/referential/package.json && npm pack dist/referential/",
    "package:all": "npm run package:portal &&  npm run package:identity  && npm run package:referential && npm run package:archive-search && npm run package:collect  && npm run package:pastis && npm run package:ingest",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:ci": "prettier --check --ignore-unknown .",
    "prepare": "cd ../../ && husky install ui/ui-frontend/.husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "11.2.14",
    "@angular/compiler": "11.2.14",
    "@angular/core": "11.2.14",
    "@angular/forms": "11.2.14",
    "@angular/material": "^11.2.13",
