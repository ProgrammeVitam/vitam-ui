{
  "name": "ui-frontend-common",
  "version": "2.1.0",
  "main": "src/index.ts",
  "pki": {
    "path": "../../dev-deployment/environments/certs/server/hosts/localhost",
    "asset": "reverse"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --proxy-config proxy.conf.json --disable-host-check --ssl --ssl-key $npm_package_pki_path/$npm_package_pki_asset.key --ssl-cert $npm_package_pki_path/$npm_package_pki_asset.crt",
    "build": "ng build --prod --build-optimizer=false --optimization=false",
    "build:dev": "ng build --prod --sourceMap=false --build-optimizer=false --optimization=false",
    "build:prod": "export NODE_OPTIONS=--max_old_space_size=4096; ng build --prod",
    "analyze": "ng build --prod --stats-json ; webpack-bundle-analyzer dist/ui-frontend-common/stats.json",
    "test": "ng test --watch=false",
    "test:conf-ci": "ng test --watch=false --karma-config=karma.conf.ci.js",
    "test:sonar": "ng test --code-coverage --watch=false --karma-config=karma.conf.ci.js --source-map=true",
    "lint": "ng lint --format=stylish",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p package.json && npm run copy-scss && npm run copy-assets",
    "packagr:tar": "ng-packagr -p package.json && npm run copy-scss && npm run copy-assets && npm run tar",
    "copy-scss": "cp-cli ./src/sass ./dist/sass",
    "copy-assets": "cp-cli ./src/assets ./dist/assets",
    "tar": "npm pack ./dist",
    "check": "npm run lint && npm test && npm run build",
    "publish": "npm publish dist"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@angular/material-moment-adapter",
      "lodash-es",
      "moment",
      "moment-mini",
      "@ngx-translate/core",
      "@ngx-translate/http-loader",
      "ngx-translate-multi-http-loader"
    ]
  },
  "private": false,
  "peerDependencies": {
    "@angular/animations": "10.1.3",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "10.1.3",
    "@angular/compiler": "10.1.3",
    "@angular/core": "10.1.3",
    "@angular/forms": "10.1.3",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "10.1.3",
    "@angular/platform-browser-dynamic": "10.1.3",
    "@angular/router": "10.1.3",
    "bootstrap": "^4.5.0",
    "bufferutil": "^4.0.1",
