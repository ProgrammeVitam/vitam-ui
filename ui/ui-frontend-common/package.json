    "ngx-quicklink": "^0.2.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "tslib": "^1.13.0",
    "underscore": "^1.10.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2",
    "material-design-icons": "^3.0.1"
  },
  "dependencies": {
    "@angular/material-moment-adapter": "^10.2.3",
