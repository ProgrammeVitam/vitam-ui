ME_CONFIG_MONGODB_URL="mongodb://{{ mongod_dbuser_admin }}:{{ mongod_dbpwd_admin }}@{{ mongod_cluster_hosts_list }}/?replicaSet={{ mongod_replicaset_name}}"
ME_CONFIG_SITE_BASEURL="{{ mongo_express_conf.baseuri | default('/') }}"
