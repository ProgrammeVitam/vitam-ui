VCAP_APP_HOST="{{ ip_admin }}"
VCAP_APP_PORT="{{ mongo_express_conf.listen_port}}"
ME_CONFIG_BASICAUTH_USERNAME="{{ mongod_express_basicauth_username }}"
ME_CONFIG_BASICAUTH_PASSWORD="{{ mongod_express_basicauth_password }}"
