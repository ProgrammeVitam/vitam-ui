{
  "service": {
    "name": "{{ logstash.service_name | default('vitamui-logstash') }}",
    "port": {{ logstash.port }},
    "address": "{{ ip_admin }}",
    "enable_tag_override": false,
    "tags": ["logstash"],
    "checks": [
      {
        "notes": "Syslog tcp port opened",
        "tcp": "{{ ip_admin }}:{{ logstash.port }}",
        "interval": "{{ logstash.check_consul | default('10') }}s"
      }
    ]
  }
}
